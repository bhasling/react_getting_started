{
  "name": "react_getting_started",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "format": "prettier --write 'src/**/*.js'"
  },
  "nodemonConfig": {
    "delay": 500,
    "ext": "js,json",
    "watch": [
      "src/"
    ],
    "ignore": []
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "bracketSpacing": true,
    "arrowParens": "always",
    "semi": true,
    "singleQuote": false
  },
  "dependencies": {
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",

    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdi/js": "^6.7.96",
    "@mdi/react": "^1.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.66",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "material-ui-image": "^3.3.2",

    "react-leaflet": "3.2.5",
    "leaflet": "^1.7.1",
    "leaflet-defaulticon-compatibility": "^0.1.1",

    "react-hooks-global-state": "1.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2"
  }
}
