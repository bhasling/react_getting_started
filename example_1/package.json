{
  "name": "react_getting_started",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "format": "prettier --write 'src/**/*.js'"
  },
  "nodemonConfig": {
    "delay": 500,
    "ext": "js,json",
    "watch": [
      "src/"
    ],
    "ignore": []
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "bracketSpacing": true,
    "arrowParens": "always",
    "semi": true,
    "singleQuote": false
  },
  "dependencies": {
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2"
  }
}
